{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "backend/app.js"
    },
    {
      "src": "/register",
      "dest": "backend/app.js"
    },
    {
      "src": "/emercado-api/(.*)",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
